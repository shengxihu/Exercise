<?xml version="1.0" encoding="UTF-8"?>
<!--DTD 定义-->
<!DOCTYPE 公司[
<!--元素定义-->
<!ELEMENT 公司 (雇员)+>
<!ELEMENT 雇员 (姓名，雇佣日期，项目清单，说明)>
<!ELEMENT 姓名 (#PCDATA)>
<!ELEMENT 雇佣日期 (#PCDATA)>
<!ELEMENT 项目清单 (项目)*>
<!ELEMENT 项目 (产品，价格)>
<!ELEMENT 产品 (#PCDATA)>
<!ELEMENT 价格 (#PCDATA)>
<!--属性定义-->
<!ATTLIST 项目清单 数量 CDATA #REQUIRED>
<!ATTLIST 雇员
	部门 CDATA #IMPLIED
	全职 CDATA #IMPLIED
>
<!ENTITY note "这里是备注信息">
]>
<!--内容定义-->
<公司>
	<雇员 部门="市场部" 全职="是">
		<姓名>李东</姓名>
		<雇佣日期>2005-01-15</雇佣日期>
		<项目清单 数量="2">
			<项目>
				<产品>打印机</产品>
				<价格>$111.00=""</价格>
			</项目>
			<项目>
				<产品>笔记本</产品>
				<价格>$998.00</价格>
			</项目>
		</项目清单>
		<说明>这里是备注信息</说明>
	</雇员>
	<雇员 部门="客服部">
		<姓名>王权</姓名>
		<雇佣日期>2005-10-2</雇佣日期>
		<项目清单 数量="1">
			<项目>
				<产品>扫描仪</产品>
				<价格>$200.00=""</价格>
			</项目>
		</项目清单>
		<说明>这里是备注信息</说明>
	</雇员>
</公司>